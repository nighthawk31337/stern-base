                                                <entry name="exec_arg">
                                                        <local_schema short_desc="Exec аргумент за стандарден терминал">
                                                                <longdesc>Еxec аргументот за употреба со стандардната терминална апликација.</longdesc>
                                                        </local_schema>
                                                </entry>
