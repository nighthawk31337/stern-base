                <dir name="system">
                        <dir name="smb">
                                <entry name="workgroup">
                                        <local_schema short_desc="SMB 群組">
                                                <longdesc>使用者所屬的 Windows 網路群組或網域。新的群組可能要等使用者再登入後才能完全生效，</longdesc>
                                        </local_schema>
                                </entry>
                        </dir>
                        <dir name="proxy">
                                <entry name="autoconfig_url">
                                        <local_schema short_desc="代表自動代理伺服器設定的 URL">
                                                <longdesc>提供代理伺服器設置值的 URL</longdesc>
                                        </local_schema>
                                </entry>
                                <entry name="socks_port">
                                        <local_schema short_desc="SOCKS 代理伺服器埠號">
                                                <longdesc>在“/system/proxy/socks_host’中定義的代理伺服器的埠號。</longdesc>
                                        </local_schema>
                                </entry>
                                <entry name="socks_host">
                                        <local_schema short_desc="SOCKS 代理伺服器主機名稱">
                                                <longdesc>socks 代理伺服器主機名稱。</longdesc>
                                        </local_schema>
                                </entry>
                                <entry name="ftp_port">
                                        <local_schema short_desc="FTP 代理伺服器埠號">
                                                <longdesc>在“/system/proxy/ftp_host’中定義的代理伺服器的埠號。</longdesc>
                                        </local_schema>
                                </entry>
                                <entry name="ftp_host">
                                        <local_schema short_desc="FTP 代理伺服器主機名稱">
                                                <longdesc>FTP 代理伺服器主機名稱。</longdesc>
                                        </local_schema>
                                </entry>
                                <entry name="secure_port">
                                        <local_schema short_desc="安全 HTTP 代理伺服器埠號">
                                                <longdesc>在“/system/proxy/secure_host’中定義的代理伺服器的埠號。</longdesc>
                                        </local_schema>
                                </entry>
                                <entry name="secure_host">
                                        <local_schema short_desc="安全 HTTP 代理伺服器主機名稱">
                                                <longdesc>安全 HTTP 代理伺服器主機名稱。</longdesc>
                                        </local_schema>
                                </entry>
                                <entry name="mode">
                                        <local_schema short_desc="代理伺服器設置模式">
                                                <longdesc>選擇代理伺服器設置模式。支援的值有“none”、“manual”、“auto”。</longdesc>
                                        </local_schema>
                                </entry>
                        </dir>
                        <dir name="http_proxy">
                                <entry name="ignore_hosts">
                                        <local_schema short_desc="不使用代理伺服器的主機">
                                                <longdesc>這設定鍵中包含了不是通過代理伺服器（如果它有啟用），而是使用直接連線的主機清單。設定值可以是主機名稱，網域名稱（使用萬用字元如 *.foo.com），主機 IP 地址（IPv4 及 IPV6 亦可）和包括網路遮罩的網路地址（如 192.168.0.0/24）。</longdesc>
                                        </local_schema>
                                </entry>
                                <entry name="authentication_password">
                                        <local_schema short_desc="HTTP 代理伺服器密碼">
                                                <longdesc>當進行 HTTP 代理時進行認證的密碼。</longdesc>
                                        </local_schema>
                                </entry>
                                <entry name="authentication_user">
                                        <local_schema short_desc="HTTP 代理伺服器使用者名稱">
                                                <longdesc>當進行 HTTP 代理時通過認證的使用者名稱。</longdesc>
                                        </local_schema>
                                </entry>
                                <entry name="use_authentication">
                                        <local_schema short_desc="代理伺服器連線是否需要認證">
                                                <longdesc>如設定為“true”，那麼連接到代理伺服器時需要認證。使用者/密碼的組合在
“/system/http_proxy/authentication_user”和“/system/http_proxy/authentication_password”中設定。</longdesc>
                                        </local_schema>
                                </entry>
                                <entry name="port">
                                        <local_schema short_desc="HTTP 代理伺服器埠號">
                                                <longdesc>在“/system/http_proxy/host’中定義的代理伺服器的埠號。</longdesc>
                                        </local_schema>
                                </entry>
                                <entry name="host">
                                        <local_schema short_desc="HTTP 代理伺服器主機名稱">
                                                <longdesc>HTTP 代理伺服器主機名稱。</longdesc>
                                        </local_schema>
                                </entry>
                                <entry name="use_http_proxy">
                                        <local_schema short_desc="使用 HTTP 代理伺服器">
                                                <longdesc>經網際網路存取 HTTP 時啟用代理伺服器設定。</longdesc>
                                        </local_schema>
                                </entry>
                        </dir>
                        <dir name="dns_sd">
                                <entry name="extra_domains">
                                        <local_schema short_desc="找尋 DNS-SD 服務的額外網域">
                                                <longdesc>在“network:///”位置中顯示的 DNS-SD 網域清單，以逗號隔開。</longdesc>
                                        </local_schema>
                                </entry>
                                <entry name="display_local">
                                        <local_schema short_desc="如何顯示本地端的 DNS-SD 服務">
                                                <longdesc>可能值為“merged”,“separate”以及“disabled”。</longdesc>
                                        </local_schema>
                                </entry>
                        </dir>
                </dir>
        </dir>
</gconf>
